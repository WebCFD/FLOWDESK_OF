FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      surfaceFeatureExtractDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

$STL_FILENAME
{
    extractionMethod    extractFromSurface;

    extractFromSurfaceCoeffs
    {
        includedAngle   175;  // Stricter feature detection for HVAC
    }

    subsetFeatures
    {
        nonManifoldEdges       no;
        openEdges              no;
    }

    writeObj                yes;
    writeVTK                no;
}

