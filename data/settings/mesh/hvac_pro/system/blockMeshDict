// Background mesh for snappyHexMesh
// Margin added to ensure geometry fully enclosed

vertices
(
    ($XMIN $YMIN $ZMIN)
    ($XMAX $YMIN $ZMIN)
    ($XMAX $YMAX $ZMIN)
    ($XMIN $YMAX $ZMIN)
    ($XMIN $YMIN $ZMAX)
    ($XMAX $YMIN $ZMAX)
    ($XMAX $YMAX $ZMAX)
    ($XMIN $YMAX $ZMAX)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (20 20 10) simpleGrading (1 1 1)
);

